[run]
branch = True
source = src
omit = 
    tests/*
    */tests/*
    migrations/*
    sitecustomize.py
    shared/shared_python/*

[report]
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.
    def __repr__
    def __str__
    if TYPE_CHECKING:
show_missing = True
skip_covered = True
